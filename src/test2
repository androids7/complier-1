# 两数相加后打印结果
void add_print(int w,int k){
	w = w + k;
	print_int(w);
	print_br();
}
# 两数相减后打印结果
void sub_print(int a, int b){
	int tmp ;
	tmp = a - b;
	print_int(tmp);
	print_br();
}


# 相减后取绝对值打印结果
void sub_print_abs(int a, int b){
	int abs;
	a = a - b;
	if(a < 0){
		abs = 0 - a;
	}else{
		abs = a;
	};
	print_int(abs);
	print_br();
}

# 打印数和回车
void p_int_br(int w){
	print_int(w);
	print_br();
}

# 从x加到y
void gas(int x, int y){
	int t;
	t = 0;
	while(x <= y){
		 t = t + x;
		 x = x + 1;
	};
	p_int_br(t);
}


void main () {
	int i;
	int j;
	int k;
	k = 3;
	i = 5;
	j = 1;

	print_br();
	sub_print(k, i);
	print_br();
	sub_print_abs(k, i);
	print_br();
	gas(j, 100);
	print_br();
	exit(1);
}

void main ( ) {
	int x;
	if ( 10 > x ) {
		x = -22 + x * 2;
	};
}
int func ( int x ) {
	int y;
	while ( 2 + x > 22 ) {
		y = 23 - x * 3;
		x = x + 1;
	};
}
void main (int x) {
	int [4] data = { 1 , 2 , 4 , 3 } ;
	int i;
	int j;
	int temp;
	i = 0;
	temp = i * j ;
	while(i < 4){
		while(j < 4){
			if( data[i] > data[j]){
				temp = data[j];
				data[j] = data[i];
				data[i] = temp;
			};
			j = j + 1;
		};
		i = i + 1;
	};
	if ( num > id and num > num or id > num ) {
		id = 12;
	} ;
	if ( num > id and num > num or id > num ) {
		if ( id == num ) {
		id [ num ] = num + ( id [ num + id ] * ( num + id [ id ] ) ) ;
		} ;
	} else {
		while ( id == 8 ) {
			break ;
			return num ;
		} ;
	} ;
}

int id ( int id [ ] ) {
	int id ;
	id = id ( 13 ) ;
	while ( num + id > num ) {
		id = num - id * num ;
		id = id + num ;
	} ;
}