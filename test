# 两数相加后打印结果
void add_print(int w,int k){
	w = w + k;
	print_int(w);
	print_br();
}
# 两数相减后打印结果
void sub_print(int a, int b){
	int tmp ;
	tmp = a - b;
	print_int(tmp);
	print_br();
}


# 相减后取绝对值打印结果
void sub_print_abs(int a, int b){
	int abs;
	a = a - b;
	if(a < 0){
		abs = 0 - a;
	}else{
		abs = a;
	};
	print_int(abs);
	print_br();
}


void p_int_br(int w){
	print_int(w);
	print_br();
}


void gas(int x, int y){
	int t;
	t = 0;
	while(x <= y){
		 t = t + x;
		 x = x + 1;
	};
	p_int_br(t);
}


void main () {
	int i;
	int j;
	int k;
	k = 3;
	i = 0;
	j = 0;

	print_br();
	sub_print(3, 5);
#	sub_print_abs(3, 5);
	gas(1, 100);
	exit(1);
}
